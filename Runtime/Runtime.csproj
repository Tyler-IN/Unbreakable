<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../NuGetPackage.Common.props" />

  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <AssemblyName>Unbreakable.Runtime</AssemblyName>
    <RootNamespace>Unbreakable.Runtime</RootNamespace>
    <VersionPrefix>0.1.5-unproven</VersionPrefix>
    <Description>Runtime library for https://www.nuget.org/packages/Unbreakable. This library supports the Unbreakable infrastructure and is not intended to be used directly from your code.</Description>
    <PackageTags>sandboxing</PackageTags>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
</Project>